@model IEnumerable<TILAHA.Controllers.Models.Laitteet>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.WarrantyId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Make)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Model)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SN)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Condition)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DeviceType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AddInfo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PurcDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.WarrEndDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PurchPrize)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.WarrantyId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Make)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Model)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SN)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Condition)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DeviceType)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AddInfo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PurcDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.WarrEndDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PurchPrize)
        </td>
        <td>
           
            @Html.ActionLink("Delete", "Delete", new { id=item.DeviceId })
        </td>
    </tr>
}

</table>
